{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:base"
    ],
    "repositories": [
      "globallogicuki/provider-harbor"
    ],
    "customManagers": [
      {
        "customType": "regex",
        "fileMatch": [
          "Makefile"
        ],
        "matchStrings": [
          "export TERRAFORM_PROVIDER_VERSION \\?= (?<currentValue>\\d+\\.\\d+\\.\\d+)"
        ],
        "datasourceTemplate": "github-releases",
        "depNameTemplate": "goharbor/terraform-provider-harbor",
        "versioningTemplate": "semver",
        "currentValueTemplate": "{{currentValue}}"
      },
      {
        "customType": "regex",
        "fileMatch": [
          "Makefile"
        ],
        "matchStrings": [
          "export TERRAFORM_VERSION \\?= (?<currentValue>\\d+\\.\\d+\\.\\d+)"
        ],
        "datasourceTemplate": "github-releases",
        "depNameTemplate": "hashicorp/terraform",
        "versioningTemplate": "hashicorp",
        "currentValueTemplate": "{{currentValue}}"
      }
    ],
    "gomod": {
      "enabled": false
    },
    "github-actions": {
      "enabled": false
    }
  }